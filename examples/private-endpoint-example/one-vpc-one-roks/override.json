            "subnet_names": [
                "vsi-zone-1",
                "vsi-zone-2",
                "vsi-zone-3"
            ],
            "update_all_workers" : false,
            "vpc_name": "workload",
            "worker_pools": [
                {
                    "entitlement": "cloud_pak",
                    "flavor": "bx2.16x64",
                    "name": "logging-worker-pool",
                    "subnet_names": [
                        "vsi-zone-1",
                        "vsi-zone-2",
                        "vsi-zone-3"
                    ],
                    "vpc_name": "workload",
                    "workers_per_subnet": 2
                }
            ],
            "workers_per_subnet": 2
        }
    ],
    "cos": [
        {
            "buckets": [
                {
                    "endpoint_type": "public",
