                },
                {
                    "endpoint_type": "public",
                    "force_delete": true,
                    "kms_key": "slz-slz-key",
                    "name": "workload-bucket",
                    "storage_class": "standard"
                }
            ],
            "keys": [],
            "name": "cos",
            "plan": "standard",
            "random_suffix": true,
            "resource_group": "slz-service-rg",
            "use_data": false
        }
    ],
    "enable_transit_gateway": true,
