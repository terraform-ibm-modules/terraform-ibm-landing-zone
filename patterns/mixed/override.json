                            "direction": "inbound",
                            "name": "allow-ibm-inbound",
                            "source": "161.26.0.0/16"
                        },
                        {
                            "action": "allow",
                            "destination": "10.0.0.0/8",
                            "direction": "inbound",
                            "name": "allow-all-network-inbound",
                            "source": "10.0.0.0/8"
                        },
                        {
                            "action": "allow",
                            "destination": "0.0.0.0/0",
                            "direction": "outbound",
                            "name": "allow-all-outbound",
                            "source": "0.0.0.0/0"
